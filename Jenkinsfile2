pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    stages{
        stage('sonarqube scan'){
            steps{
                withSonarQubeEnv(sonarserver){
                sh 'mvn mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=engrwachu_wee16_geolocation'
            }
            }
        }
        stage('maven commands'){
            steps{
                sh 'mvn clean compile install package'
            }
        }

            
    }
}